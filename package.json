		"css-loader": "^6.7.3",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.1",
		"jsdom": "^21.1.1",
		"postcss": "^8.4.23",
		"postcss-loader": "^7.3.0",
		"postcss-nesting": "^11.2.2",
